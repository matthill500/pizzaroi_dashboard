    user: null\n    });\n      localStorage.removeItem('user');\n  }\n  \n  render(){\n    return (\n      <Container fluid style={{width:'80%'}}>\n        <Navi onLogoutSuccess={this.onLogoutSuccess} user={this.state.user} />\n        <Switch>\n          <Route exact path=\"/login\">\n              <Login onLoginSuccess={this.onLoginSuccess} />\n          </Route>\n          <Route  path=\"/register\">\n              <Register />\n          </Route>\n          <Route path=\"/dashboard\">\n              <Dashboard \n                orders={this.state.orders}\n                user={this.state.user}\n              />\n          </Route>\n          <Route path=\"/stock\">\n              <Stock \n                user={this.state.user}\n              />\n          </Route>\n        </Switch>\n      </Container>\n    );\n  }\n}\n\n\nexport default withRouter(App);\n","C:\\Users\\matth\\my-react-projects\\dashboard\\src\\Login.js",[],"C:\\Users\\matth\\my-react-projects\\dashboard\\src\\Register.js",[],"C:\\Users\\matth\\my-react-projects\\dashboard\\src\\Navi.js",[],"C:\\Users\\matth\\my-react-projects\\dashboard\\src\\Dashboard.js",["116"],"import React, { Component } from 'react';\nimport { Card, Container } from 'react-bootstrap';\nimport Spinner from 'react-bootstrap/Spinner'\nimport {Line, defaults} from 'react-chartjs-2'\nclass Dashboard extends Component {\n  render(){\n    \n    if(this.props.orders.length < 1){\n      return <Container style={{textAlign:'center', marginTop:'25em'}}><Spinner animation=\"border\" role=\"status\"><span className=\"sr-only\">Loading...</span></Spinner></Container>\n    }\n\n    const orders = this.props.orders;\n    const prices = orders.map(order => parseInt(order.price));\n\n    const totalRevenue = prices.reduce(function(total,num){\n     return total + num;\n   })\n\n   var today = new Date();\n   var dd = String(today.getDate()).padStart(2, '0');\n   var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!\n   var yyyy = today.getFullYear();\n   today = yyyy + '-' + mm + '-' + dd;\n\n   const todayOrders = orders.filter(order => order.created_at.split('T')[0] === today);\n   const todayPrices = todayOrders.map(todayOrder => parseInt(todayOrder.price));\n\n   let todayTotalRevenue = 0;\n   if(todayPrices.length >= 1){\n     todayTotalRevenue = todayPrices.reduce(function(total,num){\n      return total + num;\n     }) \n   }\n  \n\n  let months = 13;\n  let ordersForEachMonth=[];\n\n  for(let i = 1; i < months; i++){\n    if(i.toString().length === 1){\n      i = \"0\"+i;\n    }\n    const ordersPerMonth = orders.filter(order => order.created_at.split('T')[0] <= '2021-'+i+'-31' && order.created_at.split('T')[0] >= '2021-'+i+'-01');\n    let ordPerMonth = ordersPerMonth.length;\n    ordersForEachMonth.push(ordPerMonth);\n  }\n\n    return (\n      <>\n      <Card style={{ margin: '20px', width: '12rem', height: '8rem', color:'white', backgroundColor:'#db3d44',float:'left'}}>\n        <Card.Body>\n          <Card.Title style={{marginTop: '0.8em',fontSize:'16px'}}>Total Revenue</Card.Title>\n          <Card.Text>\n            €{totalRevenue}\n          </Card.Text>\n        </Card.Body>\n      </Card>\n      <Card style={{ margin: '20px', width: '12rem', height: '8rem', color:'white', backgroundColor:'#db3d44', float:'left'}}>\n        <Card.Body>\n          <Card.Title style={{marginTop: '0.8em', fontSize:'16px'}}>Total Orders</Card.Title>\n          <Card.Text>\n            {orders.length}\n          </Card.Text>\n        </Card.Body>\n      </Card>\n      <Card style={{ margin: '20px', width: '12rem',height: '8rem', color:'white', backgroundColor:'#db3d44', float:'left'}}>\n        <Card.Body>\n          <Card.Title style={{marginTop: '0.8em', fontSize:'16px'}}>Total Revenue Today</Card.Title>\n          <Card.Text>\n            €{todayTotalRevenue}\n          </Card.Text>\n        </Card.Body>\n      </Card>\n      <Card style={{ margin: '20px', width: '12rem',height: '8rem', color:'white', backgroundColor:'#db3d44', float:'left'}}>\n        <Card.Body>\n          <Card.Title style={{marginTop: '0.8em', fontSize:'16px'}}>Total Orders Today</Card.Title>\n          <Card.Text>\n            {todayOrders.length}\n          </Card.Text>\n        </Card.Body>\n      </Card>\n      <Line\n        data={{\n          labels: ['JANUARY', 'FEBURARY', 'MARCH', 'APRIL', 'MAY', 'JUNE','JULY','AUGUST','SEPTEMEBER','OCTOBER','NOVEMBER','DECEMEBER'],\n          datasets:[\n            {\n              label: 'Number of Orders Per Month',\n              data: ordersForEachMonth,\n              backgroundColor: [\n                'rgba(255, 99, 132, 0.2)',\n                'rgba(54, 162, 235, 0.2)',\n                'rgba(255, 206, 86, 0.2)',\n                'rgba(75, 192, 192, 0.2)',\n                'rgba(153, 102, 255, 0.2)',\n                'rgba(255, 159, 64, 0.2)'\n            ],\n            borderColor: [\n                'rgba(255, 99, 132, 1)',\n                'rgba(54, 162, 235, 1)',\n                'rgba(255, 206, 86, 1)',\n                'rgba(75, 192, 192, 1)',\n                'rgba(153, 102, 255, 1)',\n                'rgba(255, 159, 64, 1)'\n            ],\n            borderWidth: 2\n            },\n          ]\n        }}\n        height={250}\n        width={150}\n        options={{\n          maintainAspectRatio: false,\n          scales:{\n            yAxes:[\n              {\n                ticks: {\n                  beginAtZero: true\n                }\n              }\n            ]\n          },\n          legend:{\n            labels:{\n              fontSize:25\n            }\n          }\n        }}\n      />\n      </>\n    );\n  }\n}\n\nexport default Dashboard;\n","C:\\Users\\matth\\my-react-projects\\dashboard\\src\\Stock.js",[],["117","118"],"C:\\Users\\matth\\my-react-projects\\pizzaroi_dashboard\\src\\index.js",[],["119","120"],"C:\\Users\\matth\\my-react-projects\\pizzaroi_dashboard\\src\\App.js",["121"],"C:\\U